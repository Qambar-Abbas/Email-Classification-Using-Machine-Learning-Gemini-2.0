prefix=/opt/miniconda3/envs/inbox_cleaner
exec_prefix=${prefix}
libdir=/opt/miniconda3/envs/inbox_cleaner/lib
includedir=/opt/miniconda3/envs/inbox_cleaner/include

Name: absl_time_zone
